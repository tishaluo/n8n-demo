#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/dist/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/dist/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules:/Users/tangning/Desktop/n8n-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../../../../css-blank-pseudo@3.0.3_postcss@8.5.6/node_modules/css-blank-pseudo/dist/cli.cjs" "$@"
fi
